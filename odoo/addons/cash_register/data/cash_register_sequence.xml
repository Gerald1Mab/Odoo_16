<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="cash_receipt_sequence" model="ir.sequence">
        <field name="name"> Cash Receipt Sequence</field>
        <field name="code">cash_seq_rec</field>
        <field name="prefix">ENC/</field>
        <field name="number_increment">1</field>
        <field name="padding">3</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="cash_sequence" model="ir.sequence">
        <field name="name">Cash register Sequence</field>
        <field name="code">cash_seq</field>
        <field name="prefix">DEC/</field>
        <field name="number_increment">1</field>
        <field name="padding">3</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <!--    SEQUENCE POUR RECU DE CAISSE-->
    <record id="receipt_sequence" model="ir.sequence">
        <field name="name">Receipt Sequence</field>
        <field name="code">receipt_seq</field>
        <field name="prefix">RC/%(year)s %(month)s</field>
        <field name="number_increment">1</field>
        <field name="padding">3</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
